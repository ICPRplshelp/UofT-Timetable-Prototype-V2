ion": "undergraduate", "prerequisitesText": null, "corequisitesText": null, "exclusionsText": null, "recommendedPreparation": null, "note": null, "division": "Arts and Science, Faculty of", "breadthRequirements": ["Society and its Institutions (3)"], "distributionRequirements": ["Humanities"], "publicationSections": ["French", "First-Year Foundations"]}, "created": "2022-10-18@02:45:18.473", "modified": null, "lastSaved": "0", "primaryTeachMethod": "LEC", "faculty": {"code": "ARTSC", "name": "Faculty of Arts and Science"}, "coSec": null, "department": {"code": "FRE", "name": "Department of French"}, "title": null, "maxCredit": "0.50", "minCredit": "0.50", "breadths": [{"org": {"code": "APSC", "name": "Faculty of Applied Science & Engineering"}, "breadthTypes": [{"type": "Compl Studies", "description": "Complementary Studies", "code": "CS"}]}, {"org": {"code": "ARTSC", "name": "Faculty of Arts and Science"}, "breadthTypes": [{"type": "Humanities", "description": "Humanities", "code": "Hum"}, {"type": "Society Institutions", "description": "BR=3 Society and Its Institutions", "code": "BR=3"}]}], "notes": [{"name": "Course Note", "type": "COURSE", "content": null}], "cancelInd": null, "subscriptionTtb": "false", "subscriptionOpenData": "false", "fullyOnline": "false"}, {"id": "629df418cc54df41c2beaaf9", "name": "Love, Sex and Desire in French Literature and Cinema", "ucName": null, "code": "FCS292H1", "sectionCode": "F", "campus": "St. George", "sessions": ["20229"], "sections": [{"name": "LEC0101", "type": "Lecture", "teachMethod": "LEC", "sectionNumber": "0101", "meetingTimes": null, "firstMeeting": null, "instructors": [{"firstName": "Marie-Anne", "lastName": "Visoi"}], "currentEnrolment": "60", "maxEnrolment": "70", "subTitle": null, "cancelInd": "N", "waitlistInd": "Y", "deliveryModes": [{"session": "20229", "mode": "ASYNC"}], "currentWaitlist": "0", "enrolmentInd": null, "tbaInd": "Y", "openLimitInd": "N", "notes": [{"name": "Section Note", "type": "SECTION", "co