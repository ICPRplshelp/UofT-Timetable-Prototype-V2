istable": false, "fullyOnline": false, "primaryFull": false}, {"id": "6634eea30b7d9d3dbe57f977", "name": "Self and Other in Literature and Film", "ucName": null, "code": "FLMC44H3", "sectionCode": "S", "campus": "Scarborough", "sessions": ["20251"], "sections": [{"name": "LEC01", "type": "Lecture", "teachMethod": "LEC", "sectionNumber": "01", "meetingTimes": null, "firstMeeting": null, "instructors": null, "currentEnrolment": null, "maxEnrolment": null, "subTitle": null, "cancelInd": null, "waitlistInd": null, "deliveryModes": [], "currentWaitlist": null, "enrolmentInd": null, "tbaInd": null, "openLimitInd": null, "notes": [{"name": "Section Note", "type": "SECTION", "content": ""}], "enrolmentControls": null, "linkedMeetingSections": null}], "duration": null, "cmCourseInfo": {"description": "A study of the relation between self and other in narrative fiction. This course will examine three approaches to the self-other relation: the moral relation, the epistemological relation, and the functional relation. Examples will be chosen to reflect engagements with gendered others, with historical others, with generational others, and with cultural and national others.", "title": "Self and Other in Literature and Film", "levelOfInstruction": "undergraduate", "prerequisitesText": "Any 6.0 credits ", "corequisitesText": "", "exclusionsText": "<p>(ENGC44H3)</p>", "recommendedPreparation": "<p>[ENGA01H3 and ENGA02H3] or FLMA70H3/(ENGB70H3)</p>", "note": "", "division": "University of Toronto Scarborough", "breadthRequirements": ["Arts, Literature & Language"], "distributionRequirements": null, "publicationSections": ["English"], "cmPublicationSections": [{"section": "English", "subSections": null}]}, "created": "2024-05-31@22:36:43.973", "modified": null, "lastSaved": 0, "primaryTeachMethod": "LEC", "faculty": {"code": "SCAR", "name": "University of Toronto Scarborough"}, "coSec": {"code": "", "name": null}, "department": {"code": "ENGSC", "name": "Department of English (UTSC)"